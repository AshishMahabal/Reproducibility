    "import numpy as np\n",
    "import glob\n",
    "\n",
    "l1=glob.glob(\"indiv_dmdt/*.npy\")\n",
    "l2=glob.glob(\"indiv_dmdt_150000/*.npy\")\n",
    "l3=glob.glob(\"indiv_dmdt_160000/*.npy\")\n",
    "\n",
    "l=l1+l2+l3\n",
    "x=[]\n",
    "for i in range(len(l)):\n",
    "    x.append(np.load(l[i]))\n",
    "np.save(\"ptf_dmdts\",np.stack(x))"
